{
  "name": "@ellieproject/nellie",
  "version": "0.1.0",
  "description": "An NES Emulation demo written using the ellie.js library",
  "main": "src/nellie.js",
  "exports": {
    ".": "./src/nellie.js",
    "./*": "./src/nellie/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "coverage": "nyc --reporter html --reporter text --report-dir docs/coverage npm run test:html",
    "lint": "eslint src/ test/",
    "lint:src": "eslint src/",
    "lint:test": "eslint test/",
    "test": "npm run test:text test:html",
    "test:github": "ava --color",
    "test:text": "ava --color | tee >(sed -n 's/\\x1B\\[[0-9;]\\{1,\\}[A-Za-z]//gw docs/test.log')",
    "test:html": "ava --tap | taplet -o docs/test.html"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ellieproject/nellie.js.git"
  },
  "keywords": [
    "NES",
    "MOS 6502",
    "Low Level Emulation"
  ],
  "author": "The Ellie Project, Grant Sparks",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ellieproject/nellie.js/issues"
  },
  "homepage": "https://github.com/ellieproject/nellie.js#readme",
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "nyc": "^15.1.0",
    "taplet": "^0.2.0"
  },
  "dependencies": {
    "@ellieproject/ellie": "file:../ellie.js/ellieproject-ellie-0.1.0.tgz"
  }
}
